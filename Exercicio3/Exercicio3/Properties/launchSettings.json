{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:62402", // URL de la aplicación en IIS Express
      "sslPort": 44391 // Puerto SSL para IIS Express
    }
  },
  "profiles": {
    "http": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchUrl": "swagger", // URL de inicio cuando se ejecuta con 'dotnet run'
      "launchBrowser": true, // Abre el navegador automáticamente al iniciar
      "applicationUrl": "http://localhost:5261", // URL de la aplicación al ejecutar con 'dotnet run'
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development" // Variable de entorno para el entorno de desarrollo
      }
    },
    "https": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchUrl": "swagger", // URL de inicio cuando se ejecuta con 'dotnet run'
      "launchBrowser": true, // Abre el navegador automáticamente al iniciar
      "applicationUrl": "https://localhost:7242;http://localhost:5261", // URL de la aplicación en HTTPS y HTTP
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development" // Variable de entorno para el entorno de desarrollo
      }
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchUrl": "swagger", // URL de inicio cuando se ejecuta con IIS Express
      "launchBrowser": true, // Abre el navegador automáticamente al iniciar
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development" // Variable de entorno para el entorno de desarrollo
      }
    }
  }
}
